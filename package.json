{
  "name": "flora-client-js",
  "version": "0.5.3",
  "description": "JavaScript client for Flora based APIs",
  "scripts": {
    "lint": "eslint src/ build/ test/",
    "lint-fix": "eslint --fix src/ build/ test/",
    "prepublishOnly": "npm run clean; npm run build; npm run test",
    "clean": "node -e 'const fs = require(\"fs\"); fs.globSync(\"dist/*\").forEach((file) => fs.unlinkSync(file));'",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test:browser": "playwright test test/flora-client-browser.spec.js",
    "test:node": "node --test test/flora-client-node.spec.js test/stringify.spec.js",
    "test": "npm run test:node && npm run test:browser"
  },
  "repository": "godmodelabs/flora-client-js",
  "contributors": [
    {
      "name": "Mark Plomer",
      "email": "mark.plomer@stock3.com"
    },
    {
      "name": "Nico Kaiser",
      "email": "nico@kaiser.me"
    },
    {
      "name": "Marco Baumgartl",
      "email": "marco.baumgartl@stock3.com"
    }
  ],
  "main": "dist/flora-client.cjs",
  "exports": {
    ".": {
      "require": "./dist/flora-client.cjs"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ],
  "author": "stock3 AG",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@playwright/browser-chromium": "1.52.0",
    "@playwright/browser-firefox": "1.52.0",
    "@playwright/browser-webkit": "1.52.0",
    "@playwright/test": "1.52.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "nock": "^14.0.10",
    "playwright": "1.52.0",
    "prettier": "^3.0.3",
    "rollup": "^4.53.3"
  }
}
